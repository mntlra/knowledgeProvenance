<?xml version="1.0"?>
<rdf:RDF xmlns="http://gda.dei.unipd.it/cecore/ontology/"
     xml:base="http://gda.dei.unipd.it/cecore/ontology/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://gda.dei.unipd.it/cecore/ontology/">
        <owl:imports rdf:resource="https://w3id.org/PROV-K/ontology/1.0/schema"/>
        <dc:title xml:lang="en">CORE Ontology for Nanopub Serialization</dc:title>
        <terms:creator rdf:resource="https://orcid.org/0000-0002-0676-682X"/>
        <terms:creator rdf:resource="https://orcid.org/0000-0003-0362-5893"/>
        <terms:creator rdf:resource="https://orcid.org/0000-0003-4970-4554"/>
        <terms:description xml:lang="en">This is the ontology supporting the KB creation process for Gene Disease Associations extracted from the literature and the serialization of the extracted GCS as nanopubs.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <rdfs:comment xml:lang="en">This is the ontology supporting the KB creation process for Gene Disease Associations extracted from the literature and the serialization of the extracted GCS as nanopubs.

The focus is on cancer diseases.</rdfs:comment>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://gda.dei.unipd.it/cecore/ontology/contains -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/contains">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/extractedFrom"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/NCIT_C47902"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gda.dei.unipd.it/cecore/ontology/contains"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;contains&apos; expresses the association between the Article and its Sentence.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/diseaseMentionedBy -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/diseaseMentionedBy">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/mentionsDisease"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.ebi.ac.uk/ols/ontologies/doid/Disease"/>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;diseaseMentionedBy&apos; expresses the association between the mentioned Disease and the Sentence.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/expressedBy -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/expressedBy">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/expresses"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/SO_0000704"/>
        <dc:creator>Stfano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;expressedBy&apos; relates a GCS to a Gene. A GCS has a unique related Gene.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/expresses -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/expresses">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/SO_0000704"/>
        <rdfs:range rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <dc:creator>Stfano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;expresses&apos; relates a Gene to a GCS. A Gene can have zero, one, or many GCS associated with it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/extractedFrom -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/extractedFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/NCIT_C47902"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;extractedFrom&apos; expresses the association between the Sentence and the Article it has been extracted from.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/geneMentionedBy -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/geneMentionedBy">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/mentionsGene"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/SO_0000704"/>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;geneMentionedBy&apos; expresses the association between the mentioned Gene and the Sentence.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/includedIn -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/includedIn">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/includes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/NCIT_C15426"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;includedIn&apos; expresses the association between the Sentence and the Database storing it (if any).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/includes -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/includes">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/NCIT_C15426"/>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;includes&apos; expresses the association between the Database and the Sentence stored in it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/involvedIn -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/involvedIn">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/involves"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.ebi.ac.uk/ols/ontologies/doid/Disease"/>
        <rdfs:range rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;involvedIn&apos; relates a Disease to a GCS. A Disease can have zero, one, or many GCS associated with it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/involves -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/involves">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="https://www.ebi.ac.uk/ols/ontologies/doid/Disease"/>
        <dc:creator>Stfano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;involves&apos; relates a GCS to a Disease. A GCS has a unique related Disease.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/mentionsDisease -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/mentionsDisease">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range rdf:resource="https://www.ebi.ac.uk/ols/ontologies/doid/Disease"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;mentionsDisease&apos; expresses the association between the Sentence and the mentioned Disease.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/mentionsGene -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/mentionsGene">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/SO_0000704"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;mentionsGene&apos; expresses the association between the Sentence and the mentioned Gene.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/supportedBy -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/supportedBy">
        <owl:inverseOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/supports"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gda.dei.unipd.it/cecore/ontology/supportedBy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;supportedBy&apos; expresses the association between GCS and the supporting Sentence. Each GCS is supported by at least one Sentence.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/supports -->

    <owl:ObjectProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/supports">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;supports&apos; expresses the association between the Sentence and the supported GCS. Each Sentence supports only one GCS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://gda.dei.unipd.it/cecore/ontology/AGT -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/AGT">
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&apos;AGT&apos; expresses the Active Gene Type that the gene has with regard to the specific disease. AGT can be Oncogene or Tumor Suppressor Gene. An oncogene is a gene that strenghtens the cancerous properties of a cell or tissue, whereas a tumor suppressor gene is a gene that weakens the cancerous properties of a cell or tissue.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/AGTOncogeneLikelihood -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/AGTOncogeneLikelihood">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/AGT"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Oncogene type likelihood. It is a float that specifies the likelihood for the gene of being an oncogene for the considered disease.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/AGTOncogeneSupport -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/AGTOncogeneSupport">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/AGT"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Oncogene support. It is an integer that specifies the number of evidences supporting the oncogene type of the gene with regard to the specified disease.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/AGTTSGLikelihood -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/AGTTSGLikelihood">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/AGT"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TSG type likelihood. It is a float that specifies the likelihood for the gene of being a tumor suppressor gene for the considered disease.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/AGTTSGSupport -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/AGTTSGSupport">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/AGT"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TSG support. It is an integer that specifies the number of evidences supporting the TSG type of the gene with regard to the specified disease.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/CCS -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/CCS">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
                    <rdf:Description rdf:about="http://semanticscience.org/resource/SIO_000113"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;CCS&apos; expresses the Change in the Cancer Status associated with a specific Gene. CCS can be either Progression, Regression, or Unknown.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/CCSLabel -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/CCSLabel">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/CCS"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://semanticscience.org/resource/SIO_000113"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>NOTINF</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>PROGRESSION</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>REGRESSION</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment>&apos;CCSLabel&apos; expresses the Label associated with CCS in the Sentence.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/CCSNotInformativeLikelihood -->

    <owl:DatatypeProperty rdf:about="http://gda.dei.unipd.it/cecore/ontology/CCSNotInformativeLikelihood">
        <rdfs:subPropertyOf rdf:resource="http://gda.dei.unipd.it/cecore/ontology/CCS"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://gda.dei.unipd.it/cecore/ontology/GCS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <dc:creator>Stefano Marchesin, Gianmaria Silvello</dc:creator>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cancer Not Informative Status likelihood. It is a float specifying the likelihood of having not informative cancer status in relation to Gene expression.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://gda.dei.unipd.it/cecore/ontology/CCSNotInformativeSupport -->

