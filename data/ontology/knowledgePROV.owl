    <!-- https://w3id.org/knowledgePROV/ontology/schema/assignedCertaintyDegree -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/assignedCertaintyDegree">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/AssignedTruthValue"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.0</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The assigned degree of certainty of an assertion is the probability that the information creator assigns truth value to be &quot;True&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">assignedCertaintyDegree</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/assignedCertaintyDegreeSupport -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/assignedCertaintyDegreeSupport">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/AssignedTruthValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">The support of the property  &quot;assignedCertaintyDegree&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">assignedCertaintyDegreeSupport</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/certaintyDegree -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/certaintyDegree">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/TrustedTruthValue"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.0</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The degree of certainty of an assertion is defined as the probability in which an agent assigned the trusted truth value to be &quot;True&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">certaintyDegree</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/consistencyScore -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/consistencyScore">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/ConsistencyScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">The consistency score assigned by the performed consistency test.</rdfs:comment>
        <rdfs:label xml:lang="en">consistencyScore</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/consistencyThreshold -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/consistencyThreshold">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/ConsistencyScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">Fixed threshold for the consistency test.</rdfs:comment>
        <rdfs:label xml:lang="en">coinsistencyThreshold</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/digitalSignature -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/digitalSignature">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/Signature"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="en">The digital signature of an assertion.</rdfs:comment>
        <rdfs:label xml:lang="en">digitalSIgnature</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/signatureStatus -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/signatureStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/Signature"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first xml:lang="en">Failed</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first xml:lang="en">NoSignaure</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first xml:lang="en">Verified</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The digital signature verification status of an assertion &quot;x&quot;, which may be one of three status: &quot;Verified&quot;-- the signature is verified successfully; &quot;Failed&quot;-- the signature verification is failed; and &quot;NoSignature&quot;-- do not have digital signature.</rdfs:comment>
        <rdfs:label xml:lang="en">signatureStatus</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/sufficiencyScore -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/sufficiencyScore">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/SufficiencyScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">The sufficiency score assigned by the performed sufficiency test.</rdfs:comment>
        <rdfs:label xml:lang="en">sufficiencyScore</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/sufficiencyThreshold -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/sufficiencyThreshold">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/SufficiencyScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">Fixed threshold for the sufficiency test.</rdfs:comment>
        <rdfs:label xml:lang="en">sufficiencyThreshold</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/knowledgePROV/ontology/schema/truthValue -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/knowledgePROV/ontology/schema/truthValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/knowledgePROV/ontology/schema/TruthValue"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first xml:lang="en">False</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first xml:lang="en">True</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
